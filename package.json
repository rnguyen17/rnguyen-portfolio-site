{
  "name": "frontendmasters-gatsby",
  "private": true,
  "description": "Bare-bones starter for the Gatsby Frontend Masters workshop.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc",
    "generate-icons": "npx @svgr/cli -d src/icons src/assets/icons"
  },
  "dependencies": {
    "gatsby": "^2.32.13",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-transition-group": "^4.3.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "prettier": "^1.17.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlengstorf/frontendmasters-gatsby"
  }
}
